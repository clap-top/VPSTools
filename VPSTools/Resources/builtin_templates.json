    {
      "id": "92ca8ddb-7e81-46d7-9cc7-b4399c5a3ee0",
      "name": "WordPress 博客",
      "description": "使用 Docker 部署 WordPress 博客系统（请确保Docker已安装）",
      "serviceType": "WordPress",
      "category": "建站服务",
      "commands": [
        "mkdir -p {{wordpress_directory}}",
        "cd {{wordpress_directory}} && cat > docker-compose.yml << 'EOF'\n{{wordpress_compose_config}}\nEOF",
        "cd {{wordpress_directory}} && mkdir -p wordpress-data",
        "cd {{wordpress_directory}} && mkdir -p mysql-data",
        "cd {{wordpress_directory}} && docker compose up -d",
        "echo '等待 WordPress 服务启动...'",
        "sleep 30",
        "cd {{wordpress_directory}} && docker compose ps",
        "echo 'WordPress 部署完成！访问地址: http://{{server_ip}}:{{web_port}}'"
      ],
      "configTemplate": "version: '3.8'\nservices:\n  db:\n    image: mysql:8.0\n    container_name: wordpress-mysql\n    restart: always\n    environment:\n      - MYSQL_ROOT_PASSWORD={{mysql_root_password}}\n      - MYSQL_DATABASE={{db_name}}\n      - MYSQL_USER={{db_user}}\n      - MYSQL_PASSWORD={{db_password}}\n    volumes:\n      - ./mysql-data:/var/lib/mysql\n    networks:\n      - wordpress-network\n\n  wordpress:\n    image: wordpress:{{wordpress_version}}\n    container_name: wordpress\n    restart: always\n    ports:\n      - \"{{web_port}}:80\"\n    environment:\n      - WORDPRESS_DB_HOST=db:3306\n      - WORDPRESS_DB_NAME={{db_name}}\n      - WORDPRESS_DB_USER={{db_user}}\n      - WORDPRESS_DB_PASSWORD={{db_password}}\n      - WORDPRESS_TABLE_PREFIX={{table_prefix}}\n      - WORDPRESS_DEBUG={{debug_mode}}\n    volumes:\n      - ./wordpress-data:/var/www/html\n    depends_on:\n      - db\n    networks:\n      - wordpress-network\n\nnetworks:\n  wordpress-network:\n    driver: bridge",
      "serviceTemplate": "",
